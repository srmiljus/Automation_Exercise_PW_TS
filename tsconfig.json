{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "types": ["@playwright/test"]
  },
  "include": [
    "tests/**/*.ts",
    "utils/**/*.ts",
    "pageObjects/**/*.ts",
    "testData.json"
  ]
}